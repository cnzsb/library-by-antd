{
  "name": "library-by-antd",
  "version": "0.0.1",
  "description": "A UI library based on Ant Design",
  "keywords": [
    "UI",
    "Library",
    "React",
    "AntD"
  ],
  "author": "cnzsb <cnzsb@foxmail.com>",
  "homepage": "https://github.com/cnzsb/library-by-antd",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "http://npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnzsb/library-by-antd.git"
  },
  "scripts": {
    "site": "webpack-dev-server --mode development --config ./build/webpack.site.config.js",
    "site:build": "rimraf ./_site && webpack --mode production --config ./build/webpack.site.config.js",
    "compile": "gulp compile --gulpfile ./build/gulpfile.js",
    "compile:es": "gulp compile:es --gulpfile ./build/gulpfile.js",
    "compile:lib": "gulp compile:lib --gulpfile ./build/gulpfile.js",
    "dist": "gulp dist:compile --gulpfile ./build/gulpfile.js",
    "build": "gulp build --gulpfile ./build/gulpfile.js",
    "test": "jest --config build/jest.config.js",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:watchCoverage": "npm test -- --watch --coverage"
  },
  "bugs": {
    "url": "https://github.com/cnzsb/library-by-antd.git"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "ie >= 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "antd": "~3.20.7",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "hoist-non-react-statics": "^3.3.0",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "merge2": "^1.2.3",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "through2": "^3.0.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpackbar": "^3.2.0"
  }
}
